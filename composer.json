{
    "name": "khanhartisan/laravel-backbone",
    "description": "Backbone for Laravel projects",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "KhanhArtisan\\LaravelBackbone\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KhanhArtisan\\LaravelBackbone\\Tests\\": "tests/",
            "App\\": "stubs/app/",
            "Database\\Factories\\": "stubs/database/factories/"
        }
    },
    "authors": [
        {
            "name": "Khanh",
            "email": "khanh.artisan@gmail.com"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "nunomaduro/collision": "^8.1",
        "laravel/sail": "^1.23"
    },
    "require": {
        "php": "^8.2",
        "illuminate/http": "^11.0",
        "illuminate/database": "^11.0",
        "symfony/finder": "^6.2|^7.0"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit tests"
    },
    "extra": {
        "laravel": {
            "providers": [
                "KhanhArtisan\\LaravelBackbone\\BackboneServiceProvider"
            ]
        }
    }
}
